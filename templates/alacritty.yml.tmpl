colors:
  primary:
    background: '0x{{.background.Hex}}'
    foreground: '0x{{.foreground.Hex}}'

  normal:
    black:   '0x{{.color0.Hex}}'
    red:     '0x{{.color1.Hex}}'
    green:   '0x{{.color2.Hex}}'
    yellow:  '0x{{.color3.Hex}}'
    blue:    '0x{{.color4.Hex}}'
    magenta: '0x{{.color5.Hex}}'
    cyan:    '0x{{.color6.Hex}}'
    white:   '0x{{.color7.Hex}}'

  bright:
    black:   '0x{{.color8.Hex}}'
    red:     '0x{{.color9.Hex}}'
    green:   '0x{{.color10.Hex}}'
    yellow:  '0x{{.color11.Hex}}'
    blue:    '0x{{.color12.Hex}}'
    magenta: '0x{{.color13.Hex}}'
    cyan:    '0x{{.color14.Hex}}'
    white:   '0x{{.color15.Hex}}'
